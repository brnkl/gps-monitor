sandboxed: true
version: 1.1.0
start: auto

executables:
{
  gpsService = ( location )
}

processes:
{
  envVars:
  {
    LE_LOG_LEVEL = DEBUG
  }
  run:
  {
     ( gpsService )
  }
  faultAction: restartApp
}

bindings:
{
  gpsService.location.le_posCtrl -> positioningService.le_posCtrl
  gpsService.location.le_pos -> positioningService.le_pos
}

extern:
{
  gpsService.location.brnkl_gps
}
